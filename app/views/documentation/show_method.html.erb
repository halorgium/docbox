<% content_for :sidebar, render(:partial => "method_sidebar", :locals => {:in_file => @in_file, :versions => (@versions.reverse if @versions), :object => @requested_object})%>
  <h2 id="title">
    <span class="sub"><%= @requested_object.class_type %></span>
    <%= @requested_object.name %><%= @requested_object.parameters%>
  </h2>
<%= render :partial => "show_comment", :locals => {:object => @requested_object, :comment => @comment.try(:body)} %>